{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [ "src" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)(_|test)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "destination": "./jsdoc/",
        "recurse": true
    }
}